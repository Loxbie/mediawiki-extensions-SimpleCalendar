{
	"name": "SimpleCalendar",
	"version": "2.0.0",
	"author": [
		"[http://www.organicdesign.co.nz/aran Aran Dunkley]"
	],
	"url": "http://www.mediawiki.org/wiki/Extension:SimpleCalendar",
	"description": "A simple calendar extension",
	"license-name": "[https://www.gnu.org/licenses/gpl-2.0.html GNU General Public Licence 2.0] or later",
	"type": "parserhook",
	"ExtensionMessagesFiles": {
		"SimpleCalendarMagic": "SimpleCalendar.i18n.magic.php"
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\SimpleCalendar\\" : "src/"
	},
	"Hooks": {
		"ParserFirstCallInit": "MediaWiki\\Extension\\SimpleCalendar\\Setup::onParserFirstCallInit"
	},
	"ResourceModules": {
		"ext.simplecalendar": {
			"styles": {
				"simplecalendar.css": {
					"media": "all"
				}
			}
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "resources",
		"remoteExtPath": "SimpleCalendar/resources"
	}
}
